{
  "name": "ana-loader",
  "version": "0.1.0",
  "description": "A webpack loader for analyzing dependencies. Support TypeScript, JSX, Vue, AMD, CJS, ESM, CSS, Sass, Scss, Less and Stylus.",
  "license": "MIT",
  "repository": "zjffun/ana-loader",
  "author": "zjffun",
  "homepage": "https://github.com/zjffun/ana-loader",
  "bugs": "https://github.com/zjffun/ana-loader/issues",
  "main": "./dist/index.js",
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "commitlint": "commitlint --from=master",
    "security": "npm audit",
    "lint:prettier": "prettier \"{**/*,*}.{js,json,md,yml,css,ts}\" --list-different",
    "lint:js": "eslint \"src/**/*.js\" --cache .",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "test:only": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage",
    "prepare": "npm run build",
    "release": "standard-version",
    "defaults": "webpack-defaults",
    "prepublishOnly": "yarn build && yarn test"
  },
  "files": [
    "dist/",
    "lib/",
    "index.js"
  ],
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "detective-amd": "^4.0.1",
    "detective-cjs": "^4.0.0",
    "detective-es6": "^3.0.0",
    "detective-less": "^1.0.2",
    "detective-postcss": "^6.1.0",
    "detective-sass": "^4.0.1",
    "detective-scss": "^3.0.0",
    "detective-stylus": "^2.0.1",
    "detective-typescript": "^9.0.0",
    "loader-utils": "^3.2.1",
    "schema-utils": "^4.0.0",
    "vue-template-compiler": "^2.7.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@webpack-contrib/defaults": "^6.3.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-jest": "^29.3.1",
    "codecov": "^3.8.3",
    "commitlint-azure-pipelines-cli": "^1.0.3",
    "cross-env": "^7.0.3",
    "del": "^7.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.0.4",
    "memfs": "^3.4.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "standard-version": "^9.5.0",
    "webpack": "^5.75.0"
  },
  "keywords": [
    "webpack"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "example"
    ]
  }
}
